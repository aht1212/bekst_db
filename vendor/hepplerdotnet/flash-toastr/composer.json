{
    "name": "hepplerdotnet/flash-toastr",
    "description": "Easy toastr.js flash notifications",
    "license": "MIT",
    "version": "5.6.1.1",
    "authors": [
        {
            "name": "Patrick Heppler",
            "email": "patrick@heppler.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "laravel/framework": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "HepplerDotNet\\FlashToastr\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
    "laravel": {
        "providers": [
            "HepplerDotNet\\FlashToastr\\FlashServiceProvider"
        ],
        "aliases": {
            "Flash": "HepplerDotNet\\FlashToastr\\Flash"
        }
    }
},
    "minimum-stability": "dev"
}

